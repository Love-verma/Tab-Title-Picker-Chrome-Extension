{
    "manifest_version": 3,
    "name": "Tab Title Picker",
    "version": "1.1",
    "description": "Display the current tab's title in the popup.",
    "permissions": ["tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "128": "icons/icon128.png"
        
      }
    },
    "icons": {
     "16": "icons/icon16.png",
     "32": "icons/icon32.png",
     "128": "icons/icon128.png"
    }
  }
  